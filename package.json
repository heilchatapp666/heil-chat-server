{
  "name": "elysia-vercel",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src",
    "format": "bunx biome format --write",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:studio": "bunx drizzle-kit studio",
    "db:seed": "bun run src/db/seeder --env-file=.env",
    "db:destroy": "bun run src/db/destroyer --env-file=.env",
    "auth:generate": "bunx @better-auth/cli generate --config src/lib/auth.ts --output src/db/schema/auth-schema.ts -y"
  },
  "dependencies": {
    "@faker-js/faker": "^10.2.0",
    "@hono/zod-validator": "^0.7.6",
    "@neondatabase/serverless": "^1.0.2",
    "better-auth": "^1.4.18",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.7",
    "pg": "^8.18.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@types/pg": "^8.16.0",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.21.0"
  },
  "module": "src/index.js"
}
